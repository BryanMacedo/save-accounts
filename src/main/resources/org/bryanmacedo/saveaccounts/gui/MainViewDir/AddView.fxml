<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="300.0" minHeight="300.0" prefHeight="300.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guiClasses.Controllers.MainViewDir.AddViewController">
   <center>
      <VBox fx:id="vbAccordion" alignment="CENTER" prefHeight="485.0" prefWidth="319.0">
         <children>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Label text="Titulo: " />
                  <TextField prefHeight="25.0" prefWidth="165.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Label text="Login: " />
                  <TextField prefHeight="25.0" prefWidth="165.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Label text="Senha: " />
                  <TextField prefHeight="25.0" prefWidth="165.0" />
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
            <Button mnemonicParsing="false" text="Button">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </center>
   <top>
      <HBox prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btBack" mnemonicParsing="false" onAction="#onBtBackClick" text="Voltar">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
</BorderPane>
